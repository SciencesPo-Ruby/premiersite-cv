<p id="notice"><%= notice %></p>

<h1>Projets</h1>

<table>
  <thead>
    <tr>
      <th>Date debut</th>
      <th>Date fin</th>
      <th>Titre</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projets.each do |projet| %>
      <tr>
        <td><%= projet.date_debut %></td>
        <td><%= projet.date_fin %></td>
        <td><%= projet.titre %></td>
        <td><%= projet.description %></td>
        <td><%= link_to 'Show', projet %></td>
        <td><%= link_to 'Edit', edit_projet_path(projet) %></td>
        <td><%= link_to 'Destroy', projet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Projet', new_projet_path %>
