<p id="notice"><%= notice %></p>

<h1>Experiences</h1>

<table>
  <thead>
    <tr>
      <th>Date debut</th>
      <th>Date fin</th>
      <th>Titre</th>
      <th>Entreprise</th>
      <th>Description</th>
      <th>Lieu</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @experiences.each do |experience| %>
      <tr>
        <td><%= experience.date_debut %></td>
        <td><%= experience.date_fin %></td>
        <td><%= experience.titre %></td>
        <td><%= experience.entreprise %></td>
        <td><%= experience.description %></td>
        <td><%= experience.lieu %></td>
        <td><%= link_to 'Show', experience %></td>
        <td><%= link_to 'Edit', edit_experience_path(experience) %></td>
        <td><%= link_to 'Destroy', experience, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Experience', new_experience_path %>
